<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileOne"
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:layout_marginTop="72dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileTwo"
        android:layout_width="85dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toEndOf="@+id/roadMapTileOne"
        app:layout_constraintTop_toTopOf="@+id/roadMapTileOne" />

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileThree"
        android:layout_width="85dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toEndOf="@+id/roadMapTileTwo"
        app:layout_constraintTop_toTopOf="@+id/roadMapTileTwo" />

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileFour"
        android:layout_width="85dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toEndOf="@+id/roadMapTileThree"
        app:layout_constraintTop_toTopOf="@+id/roadMapTileThree" />

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileFive"
        android:layout_width="85dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@+id/roadMapTileFour"
        app:layout_constraintTop_toBottomOf="@+id/roadMapTileFour" />

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileSix"
        android:layout_width="85dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@+id/roadMapTileFive"
        app:layout_constraintTop_toBottomOf="@+id/roadMapTileFive"/>

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileSeven"
        android:layout_width="85dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toEndOf="@+id/roadMapTileSix"
        app:layout_constraintTop_toBottomOf="@+id/roadMapTileSix" />

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileEight"
        android:layout_width="85dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/roadMapTileSeven"
        app:layout_constraintEnd_toStartOf="@+id/roadMapTileSeven"/>

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileNine"
        android:layout_width="85dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/roadMapTileEight"
        app:layout_constraintEnd_toStartOf="@+id/roadMapTileEight" />

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileTen"
        android:layout_width="85dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/roadMapTileNine"
        app:layout_constraintEnd_toStartOf="@+id/roadMapTileNine" />

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileEleven"
        android:layout_width="85dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@+id/roadMapTileTen"
        app:layout_constraintTop_toBottomOf="@+id/roadMapTileTen" />

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileTwelve"
        android:layout_width="85dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@+id/roadMapTileEleven"
        app:layout_constraintTop_toBottomOf="@+id/roadMapTileEleven" />

    <com.GrowthPlus.customViews.RoadMapTile
        android:id="@+id/roadMapTileThirteen"
        android:layout_width="85dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/roadMapTileTwelve"
        app:layout_constraintStart_toEndOf="@+id/roadMapTileTwelve" />

    <ImageView
        android:id="@+id/lock"
        android:layout_width="80dp"
        android:layout_height="85dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.435"
        android:src="@drawable/ic_baseline_lock_24" />

</androidx.constraintlayout.widget.ConstraintLayout>